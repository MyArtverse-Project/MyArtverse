{
  "name": "myfursona",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://github.com/MyFursona-Project/MyFursona",
  "workspaces": [
    "apps/*",
    "config/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "turbo dev --filter=website",
    "build:web": "turbo build --filter=website",
    "dev:api": "turbo dev --filter=api",
    "build:api": "turbo build --filter=api",
    "build": "turbo build",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^2.8.3",
    "turbo": "^1.10.12"
  },
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 8.0.0"
  }
}
